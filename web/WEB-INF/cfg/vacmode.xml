<vacmodes>
	<!-- 
		Die einzelnen Vacation-Zeiten
			'dauer' -> die Dauer, die der Vacationmodus laufen soll in Ticks
			'vacwait'  -> Die Anzahl an Ticks, die es dauert, bis der Vacationmodus aktiviert wird
	-->
	<vacmode dauer="35" vacwait="14" />
	<vacmode dauer="49" vacwait="14" />
	<vacmode dauer="98" vacwait="14" />
	<vacmode dauer="147" vacwait="14" />
	<vacmode dauer="196" vacwait="14" />
</vacmodes>